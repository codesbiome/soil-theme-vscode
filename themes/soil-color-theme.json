  "tokenColors": [
    // Soil Grammar Scopes
    {
      "name": "Soil keyword Operator Logical",
      "scope": "keyword.operator.logical.soil",
      "settings": {
        "foreground": "#CF7C7C"
      }
    },
    {
      "name": "Soil Function Param Type",
      "scope": "function.parameter.type.soil",
      "settings": {
        "foreground": "#CF7C9F"
      }
    },
    // End Grammar Scopes
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#928a83"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "variable.other.object"
      ],
      "settings": {
        "foreground": "#92b4b1"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#aca490"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#e3798b"
      }
    },
    // Custom Color Changes
    {
      "name": "Comment Keywords",
      "scope": ["comment.block.documentation keyword.other"],
      "settings": {
        "foreground": "#9a836f"
      }
    },
    {
      "name": "Punctuations",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#9c9c9c"
      }
    },
    {
      "name": "Punctuation Section Embed",
      "scope": [
        "punctuation.section.embedded",
        "meta.tag.metadata.doctype entity.name.tag"
      ],
      "settings": {
        "foreground": "#3a9c93"
      }
    },
    {
      "name": "Punctuation Defination Variable",
      "scope": [
        "punctuation.definition.variable",
        "punctuation.definition.string",
        "keyword.operator.heredoc",
        "constant.other.color.rgb-value.hex",
        "variable.other.property",
        "meta.return.type entity.name",
        "variable.scss"
      ],
      "settings": {
        "foreground": "#3a9c93"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "support.function",
        "keyword.other",
        "source.css keyword.operator.logical",
        "keyword.operator.expression.typeof"
      ],
      "settings": {
        "foreground": "#cf7c9f"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#858585"
      }
    },
    {
      "name": "Namespace",
      "scope": [
        "entity.name.type.namespace",
        "support.other.namespace",
        "meta.attribute.name entity.other.attribute-name",
        "keyword.operator.new"
      ],
      "settings": {
        "foreground": "#d39a69"
      }
    },
    {
      "name": "Entity Inherit",
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#59ae8b"
      }
    },
    {
      "name": "Storage Function",
      "scope": ["storage.type.function", "support.function.magic"],
      "settings": {
        "foreground": "#cf7c7c"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "constant.other.color",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "keyword.other.template",
        "keyword.other.substitution",
        "support.type.vendored.property-name"
      ],
      "settings": {
        "foreground": "#9c9c9c"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#cf7c9f"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "keyword.other.special-method",
        "meta.attribute.data-x entity.other.attribute-name",
        "meta.attribute.unrecognized entity.other.attribute-name",
        "meta.tag.metadata.doctype entity.other.attribute-name",
        "entity.other.attribute-name.id",
        "entity.other.attribute-name.id punctuation.definition.entity"
      ],
      "settings": {
        "foreground": "#c1a860"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other", "meta.arrow variable.parameter"],
      "settings": {
        "foreground": "#92b4b1"
      }
    },
    {
      "name": "Other Variable, String Link",
