  "tokenColors": [
    // Soil Grammar Scopes
    {
      "name": "Soil keyword Operator Logical",
      "scope": "keyword.operator.logical.soil",
      "settings": {
        "foreground": "#CF7C7C"
      }
    },
    {
      "name": "Soil Function Param Type",
      "scope": "function.parameter.type.soil",
      "settings": {
        "foreground": "#CF7C9F"
      }
    },
    // End Grammar Scopes
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#928a83"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#92b4b1"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#aca490"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    // Custom Color Changes
    {
      "name": "Comment Keywords",
      "scope": ["comment.block.documentation keyword.other"],
      "settings": {
        "foreground": "#9a836f"
      }
    },
    {
